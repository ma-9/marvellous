{
  "name": "marvellous",
  "version": "1.0.0",
  "description": "Blogging Platform for Developers",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix frontend",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ma-9/marvellous.git"
  },
  "keywords": [
    "MongoDB",
    "Express",
    "React",
    "Node",
    "NPM",
    "Git",
    "Github",
    "Heroku"
  ],
  "author": "ma-9 (Manav Oza)",
  "license": "BBS",
  "bugs": {
    "url": "https://github.com/ma-9/marvellous/issues"
  },
  "homepage": "https://github.com/ma-9/marvellous#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.7.12",
    "concurrently": "^5.0.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
